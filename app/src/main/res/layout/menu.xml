                android:layout_width="match_parent"


                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/diagnosis_img"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:tint="@color/purple_700"
                    android:src="@drawable/stethoscopeikcon"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"/>

                <TextView
                    android:id="@+id/diagnosis_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_toEndOf="@+id/diagnosis_img"
                    android:layout_toRightOf="@+id/diagnosis_img"
                    android:text="@string/diagnosis"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </RelativeLayout>
            <View
                android:id="@+id/diagnosis_view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"/>
            <RelativeLayout
