    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/dashboard_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/home_img"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/home"
                    android:tint="@color/purple_700"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"/>

                <TextView
                    android:id="@+id/home_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_toEndOf="@+id/home_img"
                    android:layout_toRightOf="@+id/home_img"
                    android:text="@string/home"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </RelativeLayout>
            <View
                android:id="@+id/das_view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"/>
            <RelativeLayout
                android:id="@+id/my_profile_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/home_img"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/profile"
                    android:layout_marginStart="10dp"
                    android:tint="@color/purple_700"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"/>

                <TextView
                    android:id="@+id/profile_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"

                    android:layout_marginBottom="10dp"
                    android:layout_toEndOf="@+id/home_img"
                    android:layout_toRightOf="@+id/home_img"
                    android:text="@string/my_profile"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </RelativeLayout>
            <View
                android:id="@+id/das_view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"/>

            <RelativeLayout
                android:id="@+id/profile_layout"
                android:layout_width="match_parent"


                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/health_directry_img"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:tint="@color/purple_700"
                    android:layout_marginStart="10dp"
                    android:src="@drawable/heart_beat"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    tools:ignore="UseAppTint" />

                <TextView
                    android:id="@+id/health_dir_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_toEndOf="@+id/health_directry_img"
                    android:layout_toRightOf="@+id/health_directry_img"
                    android:text="@string/health_directory_sec"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </RelativeLayout>
            <View
                android:id="@+id/profile_view_dr"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"/>
